-- [[ Switch Hub - Fly Only Loader ]]
local baseUrl = "https://raw.githubusercontent.com/longgamer998-code/long/refs/heads/main/"

-- Tải và chạy module Fly trực tiếp từ GitHub
local function loadFly()
    local success, content = pcall(function()
        return game:HttpGet(baseUrl .. "Fly.lua")
    end)
    
    if success then
        loadstring(content)()
        print("✓ [Switch Hub] Fly System Loaded!")
    else
        warn("❌ Không thể tải Fly.lua từ GitHub!")
    end
end

loadFly()
