-- [[ Switch Hub - Fly Loader ]]
local baseUrl = "https://raw.githubusercontent.com/longgamer998-code/long/refs/heads/main/"

local function loadFly()
    -- Kết hợp baseUrl với tên file Fly.lua
    local success, content = pcall(game.HttpGet, game, baseUrl .. "Fly.lua")
    
    if success and content and not content:find("404") then
        loadstring(content)() -- Chạy code Fly
        print("✓ [Switch Hub] Fly System Loaded Successfully!")
    else
        warn("❌ Lỗi: Không thể tải Fly.lua từ GitHub")
    end
end

loadFly()
